{
	"name": "kit-template-default",
	"private": true,
	"homepage": "https://brandontrinh.github.io/btrinh-bio-website/",

	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"deploy": "npm run build && npx gh-pages -d docs",
