		"autoprefixer": "^10.4.21",
		"gh-pages": "^6.3.0",
		"postcss": "^8.5.6",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
